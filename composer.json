{

	"name": "Devclub backlog",
	"authors": [
		{
			"name": "Artjom Kurapov",
			"email": "artkurapov@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"smarty/smarty": "3.*",
		"twitter/bootstrap": ">=2.1",
		"jquery/jquery": ">=1.7.2",
		"backbonejs/backbonejs": ">=0.9.2",
		"backbonejs/underscorejs": ">=1.3.3",
		"jquery/jquery-ui": ">=1.8.22",
		"Gratheon/Core": "1.0"
	},

	"autoload": {
		"psr-0": {
			"Gratheon": "vendor"
		}
	},

	"repositories": [

		{
			"type": "package",
			"package": {
				"version": "2.1",
				"license": "Apache-2.0",
				"name": "twitter/bootstrap",
				"dist": {
					"url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"version": "1.0",
				"license": "Apache-2.0",
				"name": "Gratheon/Core",
				"dist": {
					"url": "https://github.com/Gratheon/core/zipball/master",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery",
				"license": ["GPL-2.0", "MIT"],
				"version": "1.7.2",
				"dist": {
					"url": "http://code.jquery.com/jquery-1.7.2.js",
					"type": "file"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "backbonejs/backbonejs",
				"version": "0.9.2",
				"dist": {
					"url": "http://backbonejs.org/backbone-min.js",
					"type": "file"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "backbonejs/underscorejs",
				"version": "1.3.3",
				"dist": {
					"url": "http://documentcloud.github.com/underscore/underscore-min.js",
					"type": "file"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "smarty/smarty",
				"version": "3.1.11",
				"dist": {
					"type": "zip",
					"url": "http://www.smarty.net/files/Smarty-3.1.11.zip"
				},
				"source": {
					"type": "svn",
					"url": "http://smarty-php.googlecode.com/svn/",
					"reference": "tags/Smarty_3_1_11/distribution/"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jquery/jquery-ui",
				"license": "MIT",
				"version": "1.8.22",
				"dist": {
					"url": "https://github.com/jquery/jquery-ui/zipball/1.8.22",
					"type": "zip"
				}
			}
		}
	]

}
